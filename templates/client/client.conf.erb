<%= ERB.new(File.read(File.expand_path("../_header.erb",File.dirname(file)))).result(binding) -%>

ampname = <%= @ampname %>

collector {
  address    = <%= @ampcollector %>
  exchange   = amp_exchange
  routingkey = amp
  port       = 5671
  ssl        = true
  cacert     = <%= @keys_path %>/cacert.pem
  cert       = <%= @keys_path %>/cert.pem
  key        = <%= @keys_path %>/key.pem
}

